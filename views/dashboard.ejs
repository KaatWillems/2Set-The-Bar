 <div class="carousel-general-container">
    <div class="title-container">
      <h2 class="title">Trending Bars</h2>
    </div>
    <div class="carousel-container">
  
      <% bars.forEach((bar) => { %>
  
      <div class="carousel-row">
        <div class="carousel-image" style="background-image:url(<%= bar.bar.pictureUrl%>)">
  
        </div>
        <div class="carousel-text-container">
          <div class="carousel-text">
            <div class="carousel-barname"><%= bar.bar.name %></div>
            <div class="carousel-rating">
            
            <span class=stars> 
              <% bar.barrating.forEach((rating) => { %>
                <% if(rating === "*"){ %>
                  <i class="fa fa-star" aria-hidden="true" style="color: gold; font-size: 5rem;"></i>
                <% } else if (rating === "/") { %>
                  <i class="fa fa-star-half-o" aria-hidden="true" style="color: gold; font-size: 5rem;"></i>
                <% } else { %>
                  <i class="fa fa-star-o" aria-hidden="true" style="color: gold; font-size: 5rem;"></i>                
                <% } %>
              <% }) %>
            </span>
            </div>
          </div>
          <div class="carousel-heart"></div>
        </div>
      </div>
  
      <% }) %>
  
  
  
    </div>
